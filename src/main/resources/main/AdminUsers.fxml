<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane minWidth="800.0" prefHeight="550.0" prefWidth="750.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.AdminUsersController">
   <children>
      <Label alignment="CENTER" layoutX="47.0" layoutY="14.0" prefHeight="51.0" prefWidth="677.0" text="Hurtownia Sprzetu Komputerowego" textFill="WHITE">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <TableView fx:id="tableUsers" layoutX="6.0" layoutY="120.0" prefHeight="419.0" prefWidth="684.0">
        <columns>
            <TableColumn fx:id="idUserColumn" prefWidth="40.0" text="Id" />
            <TableColumn fx:id="typeColumn" prefWidth="26.0" text="Typ" />
            <TableColumn fx:id="nameColumn" prefWidth="66.0" text="Imię" />
          <TableColumn fx:id="secondNameColumn" prefWidth="87.0" text="Nazwisko" />
          <TableColumn fx:id="emailColumn" prefWidth="52.0" text="Emial" />
            <TableColumn fx:id="zipcodeColumn" minWidth="0.0" prefWidth="93.0" text="Kod Pocztowy" />
            <TableColumn fx:id="cityColumn" minWidth="0.0" prefWidth="72.0" text="Miasto" />
            <TableColumn fx:id="streetColumn" minWidth="0.0" prefWidth="83.0" text="Ulica" />
            <TableColumn fx:id="streetNumberColumn" minWidth="0.0" prefWidth="61.0" text="Nr domu" />
            <TableColumn fx:id="telephoneColumn" minWidth="0.0" prefWidth="103.0" text="Telefon" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="28.0" mnemonicParsing="false" onMouseClicked="#backToMainScene" prefHeight="25.0" prefWidth="60.0" style="-fx-background-color: #69b020; -fx-background-radius: 90;" text="Back" />
      <VBox layoutX="685.0" layoutY="120.0" prefHeight="391.0" prefWidth="109.0">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#editUser" prefHeight="50.0" prefWidth="100.0" style="-fx-background-color: #69b020; -fx-background-radius: 10; -fx-border-color: white; -fx-border-width: 3; -fx-border-radius: 10;" text="Edytuj Użytkowniak" textAlignment="CENTER" wrapText="true">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onMouseClicked="#addUser" prefHeight="50.0" prefWidth="100.0" style="-fx-background-color: #69b020; -fx-background-radius: 10; -fx-border-color: white; -fx-border-width: 3; -fx-border-radius: 10;" text="Dodaj Użytkownika" textAlignment="CENTER" wrapText="true">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onMouseClicked="#awansAdmin" prefHeight="50.0" prefWidth="100.0" style="-fx-background-color: #69b020; -fx-background-radius: 10; -fx-border-color: white; -fx-border-width: 3; -fx-border-radius: 10;" text="Nadaj Admina" textAlignment="CENTER" wrapText="true">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </VBox.margin>
            </Button>
            <Button layoutX="20.0" layoutY="130.0" mnemonicParsing="false" onMouseClicked="#dropAdmin" prefHeight="50.0" prefWidth="100.0" style="-fx-background-color: #69b020; -fx-background-radius: 10; -fx-border-color: white; -fx-border-width: 3; -fx-border-radius: 10;" text="Odbierz Admina" textAlignment="CENTER" wrapText="true">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </VBox.margin>
            </Button>
            <Button layoutX="10.0" layoutY="119.0" mnemonicParsing="false" onMouseClicked="#delateUser" prefHeight="50.0" prefWidth="100.0" style="-fx-background-color: #69b020; -fx-background-radius: 10; -fx-border-color: white; -fx-border-width: 3; -fx-border-radius: 10;" text="Usuń">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
